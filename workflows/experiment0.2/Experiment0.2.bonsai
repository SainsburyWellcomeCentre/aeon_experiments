                        <Expression xsi:type="ExternalizedMapping">
                          <Property Name="Value" DisplayName="WeightLossThreshold" Description="Below this value the weight loss alert will be raised." />
                        </Expression>
                        <Expression xsi:type="LessThanOrEqual">
                          <Operand xsi:type="FloatProperty">
                            <Value>80</Value>
                          </Operand>
                        </Expression>
                        <Expression xsi:type="WorkflowOutput" />
