                      <Property Name="Lower" Selector="MinPelletCount" />
                      <Property Name="Upper" Selector="MaxPelletCount" />
                    </PropertyMappings>
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="p3:CreateDiscreteUniform">
