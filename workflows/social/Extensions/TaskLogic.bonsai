              <Name>BlockParameters</Name>
            </Expression>
            <Expression xsi:type="SubscribeSubject">
              <Name>BlockParameters</Name>
            </Expression>
            <Expression xsi:type="scr:ExpressionCondition">
              <scr:Expression>BlockMaxPellets &lt;= 0 &amp;&amp; BlockDueTime == 0</scr:Expression>
