            </Expression>
            <Expression xsi:type="Combinator">
              <Combinator xsi:type="io:ReadAllText">
                <io:Path>TaskExample.yml</io:Path>
              </Combinator>
            </Expression>
            <Expression xsi:type="p1:DeserializeFromYaml">
              <p1:Type xsi:type="TypeMapping" TypeArguments="p1:Environment" />
            </Expression>
            <Expression xsi:type="Combinator">
              <Combinator xsi:type="p2:ValidateEnvironment" />
            </Expression>
            <Expression xsi:type="Combinator">
              <Combinator xsi:type="rx:Catch" />
            </Expression>
            <Expression xsi:type="WorkflowOutput" />
